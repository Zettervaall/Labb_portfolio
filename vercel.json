{
    "version": 2,
    "builds": [
        {
            "src": "index.html",
            "use": "@vercel/static-build"
        },
        {
            "src": "public/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/Visualizations/(.*)",
            "dest": "/public/Visualizations/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}
